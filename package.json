{
  "name": "citra",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.0.4",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
